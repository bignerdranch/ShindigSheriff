<table>
  <thead>
    <tr>
      <th>Event Name</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% if @organization.events %>
      <% @organization.events.each do |event| %>
        <tr>
          <td><%= link_to "#{event.name}", sekret_event_path(event) %></td>
          <td><%= event.date %></td>
        </tr>
      <% end %>
    <% else %>
      <div> No events </div>
    <% end %>
  </tbody>
  <%= link_to 'New Event', new_organization_event_path(@organization) %></br>
</table>

