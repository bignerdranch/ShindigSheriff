<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% if current_user.events %>
      <% current_user.events.each do |event| %>
        <tr>
          <td><%= link_to "#{event.name}", sekret_event_path(event) %></td>
          <td><%= event.date %></td>
        </tr>
      <% end %>
    <% else %>
      <div> No events </div>
    <% end %>
  </tbody>
</table>

